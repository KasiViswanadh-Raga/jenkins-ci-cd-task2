pipeline {
   agent  {
       docker  {
           image 'node:18'
        }
    }
    stages  {
        stage('install') {
             steps  {
                  sh 'npm install'
              }
         }
         stage('Test') {
              steps {
                 sh 'npm test'
               }
          }
          stage('start') {
              steps {
                  sh 'node index.js &'
               }
          }
      }
 }
